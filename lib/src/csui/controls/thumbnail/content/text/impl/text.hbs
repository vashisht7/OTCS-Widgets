{{#if displayLabel}}
  <div class="csui-thumbnail-label csui-thumbnail-text-label" aria-label="{{label}}">
    <span>{{label}}</span>
  </div>
{{/if}}
<div class="csui-thumbnail-value">
  <p class="csui-thumbnail-content-text" title="{{value}}" aria-label="{{value}}">{{value}}</p>
</div>

