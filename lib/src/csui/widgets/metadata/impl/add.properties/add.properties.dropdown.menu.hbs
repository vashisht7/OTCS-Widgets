<div class="binf-dropdown">
  <button id="{{btnId}}" type="button" class="binf-btn binf-dropdown-toggle"
          data-binf-toggle="dropdown" aria-haspopup="true" aria-expanded="false"
          aria-label="{{addTitleTooltip}}" title="{{addTitleTooltip}}">
    <span class="csui-button-icon icon-toolbarAdd" title="{{addTitleTooltip}}"></span>
  </button>
  <ul class="binf-dropdown-menu" role="menu"></ul>
</div>
<div class="csui-loading-parent-wrapper binf-disabled binf-hidden">
  <span class="csui-loading-dots-wrapper">
    <span class="csui-loading-dot"></span>
    <span class="csui-loading-dot"></span>
    <span class="csui-loading-dot"></span>
  </span>
</div>

